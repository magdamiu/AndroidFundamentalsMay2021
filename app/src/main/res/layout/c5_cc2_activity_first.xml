<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.Course5_CC2_first"
    xmlns:app="http://schemas.android.com/apk/res-auto">

   <Button
       android:id="@+id/button12"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="@string/text_one"
       android:onClick="startSecondActivity1"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent" />

   <Button
       android:id="@+id/button213"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:onClick="startSecondActivity2"
       android:layout_marginStart="56dp"
       android:text="@string/text_two"
       app:layout_constraintStart_toEndOf="@+id/button12"
       app:layout_constraintTop_toTopOf="parent" />

   <Button
       android:id="@+id/button56"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="@string/text_three"
       android:onClick="startSecondActivity3"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintHorizontal_bias="1.0"
       app:layout_constraintStart_toEndOf="@id/button213"
       app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>